{
  "name": "Star-Citizen-Assistant",
  "version": "0.0.5",
  "description": "An assistant for Star Citizen to help control your ship",
  "main": "main.js",
  "repository": "https://github.com/andreglegg/SCA",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps",
    "build": "build --win",
    "ship": "build --win -p always"
  },
  "author": "Andre Glegg",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^5.0.1",
    "electron-editor-context-menu": "^1.1.1",
    "electron-modal": "^1.0.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-settings": "^3.1.4",
    "electron-updater": "^2.21.10",
    "express": "^4.16.3",
    "ip": "^1.1.5",
    "path": "^0.12.7",
    "platform": "^1.3.5",
    "react": "^0.13.3",
    "robotjs": "^0.5.1",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "electron": "^2.0.0-beta.8",
    "electron-builder": "^20.10.0",
    "electron-packager": "^4.1.3",
    "electron-rebuild": "^1.7.3"
  },
  "build": {
    "appId": "sca.andreglegg.no",
    "mac": {
      "category": "scassistant.app.category.type"
    },
    "win": {
      "icon": "./assets/icons/win/icon.ico"
    }
  }
}
